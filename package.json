{
  "name": "devnotes",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && npm run deploy:s3 && npm run deploy:invalidate",
    "deploy:s3": "aws s3 sync dist/ s3://notes.vinny.dev --delete --cache-control \"public, max-age=31536000, immutable\" --exclude \"*.html\" --exclude \"*.json\" && aws s3 sync dist/ s3://notes.vinny.dev --delete --cache-control \"public, max-age=0, must-revalidate\" --include \"*.html\" --include \"*.json\"",
    "deploy:invalidate": "aws cloudfront create-invalidation --distribution-id E219UQK7YM96PZ --paths \"/*\""
  },
  "dependencies": {
    "@codemirror/commands": "^6.8.1",
    "@codemirror/lang-markdown": "^6.3.4",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.1",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/prismjs": "^1.26.5",
    "@types/react-window": "^1.8.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "codemirror": "^6.0.2",
    "dexie": "^3.2.4",
    "fuse.js": "^7.1.0",
    "jszip": "^3.10.1",
    "katex": "^0.16.22",
    "lucide-react": "^0.294.0",
    "mermaid": "^11.9.0",
    "prismjs": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.11",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "unified": "^11.0.5",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest-axe": "^3.5.9",
    "@types/jszip": "^3.4.0",
    "@types/katex": "^0.16.7",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "terser": "^5.43.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^1.0.4",
    "workbox-window": "^7.3.0"
  }
}
