# DevNotes Environment Configuration
# Copy this file to .env.local for local development

# AWS Configuration (for local deployment only)
# Note: In production (GitHub Actions), these are set as repository secrets
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# Deployment Configuration
S3_BUCKET=notes.vinny.dev
CLOUDFRONT_DISTRIBUTION_ID=E219UQK7YM96PZ

